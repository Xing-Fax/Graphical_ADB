        <TextBlock x:Name="Inf" HorizontalAlignment="Left" TextWrapping="Wrap" Text="未连接到设备..." VerticalAlignment="Top" Margin="530,184.76,0,0" Foreground="Black" Width="190" TextAlignment="Right"/>
        <Grid x:Name="设备" Margin="270,175,140,270">
            <TextBox x:Name="IP" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="192.168.1.107" VerticalAlignment="Top" Width="167.13" Margin="22.87,3,0,0"/>
