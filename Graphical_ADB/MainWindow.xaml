            <Button x:Name="连接" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="60" Click="Button_Click_15" Margin="195,0,0,0" Content="连接"/>
        </Grid>
        <Grid HorizontalAlignment="Left" Height="255" VerticalAlignment="Top" Width="710" Margin="10,210,0,0">
            <Rectangle HorizontalAlignment="Left" Height="255" Stroke="#FF7A7A7A" VerticalAlignment="Top" Width="250"/>
            <ScrollViewer x:Name="框" HorizontalAlignment="Left" Height="255" VerticalAlignment="Top" Width="250">
                <TextBlock x:Name="日志" TextWrapping="Wrap"/>
            </ScrollViewer>
        </Grid>
        <Grid x:Name="标题" HorizontalAlignment="Left" Height="25" VerticalAlignment="Top" Width="730" MouseMove="标题_MouseMove" Background="#00000000">
            <TextBlock HorizontalAlignment="Left" TextWrapping="Wrap" Text="ADB网络遥控器" VerticalAlignment="Top" Margin="30.703,7.22,0,0" FontSize="14"/>
            <materialDesign:PackIcon HorizontalAlignment="Left" VerticalAlignment="Top" Height="17.78" Width="15"  Margin="13.333,7.22,0,0" Kind="CameraControl"/>
            <Button
                Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}" ToolTip="关闭程序" Height="Auto" Margin="705,0,0,0" Width="Auto" Background="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click_11">
                <materialDesign:PackIcon Kind="CloseThick" />
            </Button>
            <Button
                Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}" ToolTip="最小化" Height="Auto" Margin="680,0,25,0" Width="Auto" Background="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click_12">
                <materialDesign:PackIcon Kind="WindowMinimize" />
            </Button>
            <Button
                Style="{StaticResource MaterialDesignFloatingActionMiniLightButton}" ToolTip="程序设置" Height="Auto" Margin="655,0,50,0" Width="Auto" Background="{x:Null}" BorderBrush="{x:Null}" Visibility="Collapsed">
