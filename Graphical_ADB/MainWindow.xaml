        <Image x:Name="监控" HorizontalAlignment="Left" Height="255" VerticalAlignment="Top" Width="453.278" Margin="266.722,210,0,0"/>
        <TextBlock x:Name="信息" HorizontalAlignment="Left" Height="15" Margin="595,450,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="122.5" TextAlignment="Right" Foreground="#FF9C0000"/>
